{
  "name": "prinsights",
  "description": "CLI tool to analyze GitHub pull requests for observability issues",
  "version": "0.0.1",
  "author": "Sonali Lipsa Patra",
  "bin": {
    "prinsights": "./bin/run.js"
  },
  "bugs": {
    "url": "https://github.com/Sonalilipsa17375/PR-insights/issues" 
  },
  "homepage": "https://github.com/Sonalilipsa17375/PR-insights",
  "repository": {
    "type": "git",
    "url": "https://github.com/Sonalilipsa17375/PR-insights.git" 
  },
  "keywords": [
    "oclif",
    "github",
    "cli",
    "observability",
    "pr",
    "pull-requests",
    "openai"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "type": "module",
  "types": "dist/index.d.ts",
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "./bin",
    "./dist",
    "./oclif.manifest.json"
  ],
  "oclif": {
    "bin": "prinsights",
    "dirname": "prinsights",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins"
    ],
    "topicSeparator": " ",
    "topics": {
      "hello": {
        "description": "Say hello to the world and others"
      }
    }
  },
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "lint": "eslint",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "npm run lint",
    "prepack": "oclif manifest && oclif readme",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif readme && git add README.md"
  },
  "dependencies": {
    "@oclif/core": "^4",
    "@oclif/plugin-help": "^6",
    "@oclif/plugin-plugins": "^5"
  },
  "devDependencies": {
    "@eslint/compat": "^1",
    "@oclif/prettier-config": "^0.2.1",
    "@oclif/test": "^4",
    "@types/chai": "^4",
    "@types/mocha": "^10",
    "@types/node": "^18",
    "chai": "^4",
    "eslint": "^9",
    "eslint-config-oclif": "^6",
    "eslint-config-prettier": "^10",
    "mocha": "^10",
    "oclif": "^4",
    "shx": "^0.3.3",
    "ts-node": "^10",
    "typescript": "^5"
  }
}